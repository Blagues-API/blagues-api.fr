{
  "name": "nuxt-app",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "prepare": "husky install",
    "lint:js": "eslint --ext .vue,.ts,.js .",
    "lint:js:fix": "eslint --ext .vue,.ts,.js . --fix",
    "lint:style": "stylelint \"**/*.{vue,scss,css}\"",
    "lint:style:fix": "stylelint \"**/*.{vue,css}\" --fix",
    "lint": "yarn lint:js && yarn lint:style",
    "lint:fix": "yarn lint:js:fix && yarn lint:style:fix"
  },
  "lint-staged": {
    "*.{js,ts,vue}": "eslint",
    "*.{css,scss,vue}": "stylelint"
  },
  "dependencies": {
    "@nuxtjs/device": "^3.1.0",
    "@pinia/nuxt": "^0.4.7",
    "pinia": "^2.0.33",
    "prismjs": "^1.29.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@nuxt/eslint-config": "^0.1.1",
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "@nuxtjs/eslint-module": "^4.0.2",
    "@nuxtjs/stylelint-module": "^5.0.1",
    "@types/prismjs": "^1.26.0",
    "eslint": "^8.37.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.5.0",
    "nuxt": "^3.3.3",
    "postcss-html": "^1.5.0",
    "sass": "^1.60.0",
    "stylelint": "^15.4.0",
    "stylelint-config-property-sort-order-smacss": "^9.1.0",
    "stylelint-config-standard-scss": "^7.0.1",
    "stylelint-config-standard-vue": "^1.0.0",
    "vite-plugin-prismjs": "^0.0.8",
    "vite-svg-loader": "^4.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Blagues-API/blagues-api.fr.git"
  },
  "homepage": "https://www.blagues-api.fr",
  "bugs": {
    "url": "https://github.com/Blagues-API/blagues-api.fr/issues"
  }
